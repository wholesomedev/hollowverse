{
  "extends": [
    "../../tslint.json",
    "rxjs-tslint-rules"
  ],
  "rules": {
    "match-default-export-name": false,
    "no-multiline-string": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "completed-docs": false,

    "rxjs-add": {
      "severity": "error"
    },
    "rxjs-no-add": false,
    "rxjs-no-create": true,
    "rxjs-no-unused-add": true,
    "rxjs-no-wholesale": true
  }
}